{
  "name": "pgvector",
  "version": "0.1.6",
  "description": "pgvector support for Node.js and Bun (and TypeScript)",
  "homepage": "https://github.com/pgvector/pgvector-node",
  "license": "MIT",
  "authors": [
    "ankane"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pgvector/pgvector-node"
  },
  "exports": {
    "./drizzle-orm": {
      "types": "./types/drizzle-orm/index.d.ts",
      "default": "./src/drizzle-orm/index.js"
    },
    "./knex": {
      "types": "./types/knex/index.d.ts",
      "default": "./src/knex/index.js"
    },
    "./mikro-orm": {
      "types": "./types/mikro-orm/index.d.ts",
      "default": "./src/mikro-orm/index.js"
    },
    "./pg": {
      "types": "./types/pg/index.d.ts",
      "default": "./src/pg/index.js"
    },
    "./sequelize": {
      "types": "./types/sequelize/index.d.ts",
      "default": "./src/sequelize/index.js"
    },
    "./utils": {
      "types": "./types/utils/index.d.ts",
      "default": "./src/utils/index.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "drizzle-orm": [
        "types/drizzle-orm/index.d.ts"
      ],
      "knex": [
        "types/knex/index.d.ts"
      ],
      "mikro-orm": [
        "types/mikro-orm/index.d.ts"
      ],
      "pg": [
        "types/pg/index.d.ts"
      ],
      "sequelize": [
        "types/sequelize/index.d.ts"
      ],
      "utils": [
        "types/utils/index.d.ts"
      ]
    }
  },
  "files": [
    "src",
    "types"
  ],
  "engines": {
    "node": ">= 12"
  },
  "scripts": {
    "build": "tsc",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "jest": {
    "testMatch": [
      "**/*.test.mjs"
    ],
    "transform": {}
  },
  "devDependencies": {
    "@mikro-orm/core": "^5.9.4",
    "@mikro-orm/postgresql": "^5.9.4",
    "@prisma/client": "^5.7.0",
    "drizzle-orm": "^0.29.1",
    "jest": "^29.5.0",
    "knex": "^3.1.0",
    "objection": "^3.1.3",
    "pg": "^8.6.0",
    "pg-promise": "^11.4.3",
    "postgres": "^3.3.4",
    "sequelize": "^6.6.2",
    "typeorm": "^0.3.17",
    "typescript": "^5.0.3"
  }
}
